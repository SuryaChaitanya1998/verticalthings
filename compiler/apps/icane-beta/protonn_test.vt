module protonn_test;

use arduino;

include model;
include Config;

//int scores[10];


default flow predict(float[FEATURE_LENGTH] x){
	float[ldDim] x_cap;
	float[numLabels] y_cap;
	float weight = 0.0, maxScore;
	int maxIndex;
	y_cap=0.0;
	// Project x onto ldDim dimension
	x_cap=x*ldProjectionMatrix;
	for i in [0,numPrototypes]{
		weight=0.0;
		weight=(x_cap-prototypeMatrix[i]) * (x_cap-prototypeMatrix[i]);
		weight=-1*gamma*gamma*weight;
		weight=exp(weight);
		prototypeLabelMatrix[i]=prototypeLabelMatrix[i]*weight;
		y_cap=y_cap+prototypeLabelMatrix[i];
	}
	//scores=100000*y_cap;
	maxScore= -FLT_MAX;
	maxIndex=0;
	for i in [0,numLabels]{
		if(y_cap[i] > maxScore){
			maxIndex=i;
			maxScore=labelScores[i];
		}
	}
	//next(maxIndex);
}

